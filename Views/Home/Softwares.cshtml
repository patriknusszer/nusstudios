@{
    ViewData["Title"] = "Products";
}

@section meta {
    <meta name="keywords" content="Nusstudios,softwares,open source,projects,developer" />
    <meta name="description" content="All open source projects and applications of Nusstudios" />
    <meta name="author" content="Patrik Nusszer" />
}

@section styles {
    <link rel="stylesheet" type="text/css" href="~/css/latofonts.css">
    <link rel="stylesheet" type="text/css" href="~/css/master_2.css">
    <link rel="stylesheet" type="text/css" href="~/css/itemslisting.css">
}

@section scripts {
    <script src="~/js/master.js"></script>
}


<div class="integrator">
    <div class="column">
        <p class="heading">@(Model.GB ? "Apps" : "Appok")</p>
        <a asp-controller="Softwares" asp-action="product_nuss"><p onMouseOver="showInfo('An advanced Youtube video downloader for chrome', this)" onmouseout="hideInfo(this)">Nuss Youtube downloader</p></a>		<a asp-controller="Softwares" asp-action="product_nussdesktop_javafx"><p onMouseOver="showInfo('An advanced Youtube video downloader for desktops', this)" onmouseout="hideInfo(this)">Nuss Youtube downloader JavaFx</p></a>
        <a asp-controller="Softwares" asp-action="product_nusstumblr"><p onMouseOver="showInfo('A Tumblr blog downloader', this)" onmouseout="hideInfo(this)">Tumblr Downloader</p></a>
		<a asp-controller="Softwares" asp-action="product_instarchiver"><p onMouseOver="showInfo('Instagram account archiver', this)" onmouseout="hideInfo(this)">Instagram archiver</p></a>
        <a asp-controller="Softwares" asp-action="product_imagetopdfui"><p onMouseOver="showInfo('Converts your images into a PDF doc', this)" onmouseout="hideInfo(this)">Image to PDF UI</p></a>
        <a asp-controller="Softwares" asp-action="product_gia"><p onMouseOver="showInfo('Google reverse image search results backup chrome extension', this)" onmouseout="hideInfo(this)">Google Image Archiver</p></a>
    </div>
</div>

<div class="integrator">
    <div class="column">
        <p class="heading">@(Model.GB ? "Games" : "Játékok")</p>
        <a asp-controller="Softwares" asp-action="product_cupmixer"><p onMouseOver="showInfo('Classic ball hunter', this)" onmouseout="hideInfo(this)">Cup Mixer</p></a></div>
</div>

<div class="integrator">
    <div class="column">
        <p class="heading">@(Model.GB ? "Developer & Educational" : "Fejlesztői és oktatási célu")</p>
