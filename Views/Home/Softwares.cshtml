        <a asp-controller="Softwares" asp-action="product_CPPO"><p onMouseOver="showInfo('Lightweigth object hierarchy management in C++', this)" onmouseout="hideInfo(this)">C++ Object library</p></a>
        <a asp-controller="Softwares" asp-action="product_pixie"><p onMouseOver="showInfo('A coordinate and color grabber utility', this)" onmouseout="hideInfo(this)">Pixie</p></a>
        <a asp-controller="Softwares" asp-action="product_pixie_macos"><p onMouseOver="showInfo('A coordinate and color grabber utility', this)" onmouseout="hideInfo(this)">Pixie for macOS</p></a>
        <a asp-controller="Softwares" asp-action="product_tfutilpp"><p onMouseOver="showInfo('A configuration cryptor for Transformers games', this)" onmouseout="hideInfo(this)">TF_Games_Util++</p></a>
        <a asp-controller="Softwares" asp-action="product_spltoemf"><p onMouseOver="showInfo('An .SPL to .EMF converter', this)" onmouseout="hideInfo(this)">SPL to EMF</p></a>
