@{
    ViewData["Title"] = "Image to PDF UI";
}

@section topheading {
    Image to PDF UI
}

@section subheading {
    Converts images to PDF document
}

@section description {
    This application lets you easily create PDF documents from images with a minimalist,
    straightforward UI. You can use regular expressions to organize the added images
    by showing the capturing group where the index by which the images can be reordered
    is located in the filenames.
}

