@section keywords {
    <meta name="keywords" content="Transformers, Fall of Cybertron, War for Cybertron, Rise of the Dark Spark" />
    <meta name="keywords" content="Coalesced, ini, int, configuration, decoder, encoder, encryptor, decryptor" />
    <meta name="keywords" content="xor, bacter, windows-1252, UTF-16, Unicode" />
}

@section topheading
{
    @(Model.GB ? "Transformers configs low level" : "Transformer játékok konfigurációi alacsony szinten")

}

@section subheading
{
    @(Model.GB ? "INI/INT file structures" : "INI/INT fájlszerkezetek")

}

@section description
{
    @if (Model.GB)
    {
        <article>
            @Html.Raw(Nusstudios.Common.Tools.QueryProcessed("transformers_games_configurations_low_level"))
        </article>
    }
    else
    {
        <article>
            @Html.Raw(Nusstudios.Common.Tools.QueryProcessed("transformers_games_configurations_low_level_hu"))
        </article>
    }
}