@section keywords {
    <meta name="keywords" content="heapsort, onlogn, binary heap, max min heap, heapify" />
}

@section topheading
{
    @(Model.GB ? "Kupacrendezés" : "Heapsort")
}

@section subheading
{
    @(Model.GB ? "O(nlogn) sorting algorithm" : "O(nlogn) rendezőalgoritmus")
}

@section description
{
    @if (Model.GB)
    {
        <article>
            @Html.Raw(Nusstudios.Common.Tools.QueryProcessed("heapsort"))
        </article>
    }
    else
    {
        <article>
            @Html.Raw(Nusstudios.Common.Tools.QueryProcessed("heapsort_hu"))
        </article>
    }
}

